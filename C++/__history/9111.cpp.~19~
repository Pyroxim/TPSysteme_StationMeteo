//---------------------------------------------------------------------------
#pragma hdrstop

#include <fmx.h>
#include "9111.h"

#pragma package(smart_init)
//---------------------------------------------------------------------------

9111::9111()
{

}

9111::~9111()
{

}

9111::Initialisation()
{
	IDCarte = Register_Card(PCI_9111DG, 0);

    AI_9111_Config(this->IDCarte, TRIG_INT_PACER, P9111_TRGMOD_SOFT, 0);
	AO_9111_Config(this->ID_Card, P9111_AO_UNIPOLAR);

	return this->ID_Card;
}

float 9111::Acquisition()
{
  	F64 Tension_lu;
	AI_VReadChannel(this->ID_Card,1, AD_B_10_V, &Tension_lu);
    	return Tension_lu;

}
